{
  "name": "backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "rm": "rm -rf ./prisma/migrations",
    "reset": "dotenv -e ../.env -- prisma migrate reset --schema=./prisma/schema.prisma",
    "migrate": "dotenv -e ../.env -- prisma migrate dev --name init --schema=./prisma/schema.prisma",
    "generate": "dotenv -e ../.env -- prisma generate --schema=./prisma/schema.prisma",
    "seed": "dotenv -e ../.env -- ts-node prisma/seed.ts",
    "studio": "dotenv -e ../.env -- prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"tsc -w\" \"nodemon ./dist/server.js\"",
    "insert": "ts-node scripts/insertDiseases.ts && ts-node scripts/insertMedications.ts && ts-node scripts/insertSymptoms.ts",
    "setup": "npm run rm && npm run reset && npm run migrate && npm run generate && npm run insert && npm run seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.14.1",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "uuid": "^11.1.0",
    "xml2js": "^0.6.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.14",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
